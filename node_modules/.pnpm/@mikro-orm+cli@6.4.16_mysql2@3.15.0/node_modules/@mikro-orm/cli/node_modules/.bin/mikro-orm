#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules/@mikro-orm/cli/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules/@mikro-orm/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules/@mikro-orm/cli/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules/@mikro-orm/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/@mikro-orm+cli@6.4.16_mysql2@3.15.0/node_modules:/mnt/c/Users/faust/OneDrive/Escritorio/UTN/3° Año/Desarrollo de software/tp-dsw/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli" "$@"
else
  exec node  "$basedir/../../cli" "$@"
fi
